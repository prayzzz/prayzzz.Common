<Project>

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <!--<VersionPrefix>1.0.0-unstable</VersionPrefix>-->
        <!--<VersionSuffix></VersionSuffix>-->
        <PackageLicenseUrl>https://github.com/prayzzz/prayzzz.Common/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/prayzzz/prayzzz.Common</RepositoryUrl>
        <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
        <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
        <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    </PropertyGroup>

    <Target Name="PrintVersion" BeforeTargets="BeforeBuild">
        <Message Text="Building $(AssemblyName) version $(Version)" Importance="High"/>
    </Target>

</Project>